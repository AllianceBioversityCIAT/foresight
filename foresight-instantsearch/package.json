    "eslint": "6.8.0",
    "eslint-config-algolia": "13.4.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.2",
    "html-webpack-plugin": "^5.3.2",
    "parcel-bundler": "1.12.5",
    "prettier": "1.19.1",
